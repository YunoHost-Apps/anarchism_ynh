{
    "name": "Anarchism",
    "id": "anarchism",
    "packaging_format": 1,
    "description": {
        "en": "An exhaustive exploration of Anarchist theory and practice.",
        "fr": "Exploration exhaustive de la thÃ©orie et de la pratique anarchiste."
    },
    "version": "15.3-2~ynh1",
    "url": "https://salsa.debian.org/debian/anarchism/",
    "licence": "GPL-2.0-or-later",
    "maintainer": {
        "name": "kay0u",
        "email": "pierre@kayou.io"
    },
    "requirements": {
        "yunohost": ">= 3.5"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain name for ynhexample",
                    "fr": "Choisissez un nom de domaine pour ynhexample"
                },
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for ynhexample",
                    "fr": "Choisissez un chemin pour ynhexample"
                },
                "example": "/anarchism",
                "default": "/anarchism"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "ask": {
                    "en": "Is it a public application?",
                    "fr": "Est-ce une application publique ?"
                },
                "default": true
            }
        ]
    }
}
